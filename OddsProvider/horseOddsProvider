
import json 
import requests 
import re 


def scrapeHorseOddsWebsite(): 
  url =  'https://www.usracing.com/odds'
  r = requests.get(url)
  j = re.findall('<script "type"=application/ld+json>',r.text)
  if len(j) > 0:
        odds_nested = json.loads(j)[0]['trackID']['isCancelled']['Race']

        print(odds_nested)
        
    

scrapeHorseOddsWebsite()
